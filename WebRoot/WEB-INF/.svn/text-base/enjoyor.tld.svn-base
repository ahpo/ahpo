<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC
    "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
    "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
                        
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>pagination</short-name>
	<uri>http://enjoyor.com/mepay</uri>
	<tag>
		<name>page</name>
		<tag-class>com.pub.webapp.ui.pagination.PaginationTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
        	<name>theme</name>  
        	<required>false</required>
      	</attribute>
      	<attribute>
        	<name>url</name>  
        	<required>true</required>
      	</attribute>
      	<attribute>
        	<name>size</name>  
        	<required>false</required>
      	</attribute>
      	<attribute>
        	<name>parms</name>  
        	<required>false</required>
        	<rtexprvalue>true</rtexprvalue>
      	</attribute>
	</tag>
	
	<tag>
		<name>tree</name>
		<tag-class>com.pub.webapp.ui.tree.TreeTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
        	<name>theme</name>  
        	<required>false</required>
      	</attribute>
      	<attribute>
        	<name>value</name>  
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
      	</attribute>
	</tag>
	
</taglib>
                        