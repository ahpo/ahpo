package com.easygo.dao.ejdb;

import java.util.List;

import org.apache.ibatis.annotations.Param;
import org.springframework.stereotype.Repository;

import com.easygo.vo.ejdb.CarIllegalInfo;
import com.easygo.vo.ejdb.CarIllegalInfoExample;
import com.easygo.vo.ejdb.CarIllegalInfoWithBLOBs;

@Repository
public interface CarIllegalInfoMapper {
	int countByExample(CarIllegalInfoExample example);

	int deleteByExample(CarIllegalInfoExample example);

	int deleteByPrimaryKey(Integer id);

	int insert(CarIllegalInfoWithBLOBs record);

	int insertSelective(CarIllegalInfoWithBLOBs record);

	List<CarIllegalInfoWithBLOBs> selectByExampleWithBLOBs(
			CarIllegalInfoExample example);

	/**
	 * 获取分页信息
	 * 
	 * @param example
	 * @param offset
	 *            行偏移
	 * @param pageSize
	 *            分页大小
	 * @return
	 */
	List<CarIllegalInfoWithBLOBs> selectPageByExampleWithBLOBs(
			@Param("example") CarIllegalInfoExample example,
			@Param("offset") int offset, @Param("pageSize") int pageSize);

	List<CarIllegalInfo> selectByExample(CarIllegalInfoExample example);

	CarIllegalInfoWithBLOBs selectByPrimaryKey(Integer id);

	int updateByExampleSelective(
			@Param("record") CarIllegalInfoWithBLOBs record,
			@Param("example") CarIllegalInfoExample example);

	int updateByExampleWithBLOBs(
			@Param("record") CarIllegalInfoWithBLOBs record,
			@Param("example") CarIllegalInfoExample example);

	int updateByExample(@Param("record") CarIllegalInfo record,
			@Param("example") CarIllegalInfoExample example);

	int updateByPrimaryKeySelective(CarIllegalInfoWithBLOBs record);

	int updateByPrimaryKeyWithBLOBs(CarIllegalInfoWithBLOBs record);

	int updateByPrimaryKey(CarIllegalInfo record);
}