package com.easygo.dao.ejdb;

import java.util.List;

import org.apache.ibatis.annotations.Param;
import org.springframework.stereotype.Repository;

import com.easygo.vo.ejdb.SoftVersion;
import com.easygo.vo.ejdb.SoftVersionExample;

@Repository
public interface SoftVersionMapper {
	int countByExample(SoftVersionExample example);

	int deleteByExample(SoftVersionExample example);

	int deleteByPrimaryKey(Integer id);

	int insert(SoftVersion record);

	int insertSelective(SoftVersion record);

	List<SoftVersion> selectByExampleWithBLOBs(SoftVersionExample example);

	/**
	 * 获取分页信息
	 * 
	 * @param example
	 * @param offset
	 *            行偏移
	 * @param pageSize
	 *            分页大小
	 * @return
	 */
	List<SoftVersion> selectPageByExampleWithBLOBs(
			@Param("example") SoftVersionExample example,
			@Param("offset") int offset, @Param("pageSize") int pageSize);

	List<SoftVersion> selectByExample(SoftVersionExample example);

	SoftVersion selectByPrimaryKey(Integer id);

	int updateByExampleSelective(@Param("record") SoftVersion record,
			@Param("example") SoftVersionExample example);

	int updateByExampleWithBLOBs(@Param("record") SoftVersion record,
			@Param("example") SoftVersionExample example);

	int updateByExample(@Param("record") SoftVersion record,
			@Param("example") SoftVersionExample example);

	int updateByPrimaryKeySelective(SoftVersion record);

	int updateByPrimaryKeyWithBLOBs(SoftVersion record);

	int updateByPrimaryKey(SoftVersion record);
}