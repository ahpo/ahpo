<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>
	<package name="business" namespace="/" extends="enjoyor-default">
		<action name="userQuery" class="userAction" method="userQuery">
			<result name="success">/WEB-INF/jsp/biz/user/userList.jsp</result>
		</action>
		<action name="userToUpdate" class="userAction" method="userToUpdate">
			<result name="success">/WEB-INF/jsp/biz/user/userUpdate.jsp</result>
		</action>
		<action name="userUpdate" class="userAction" method="userUpdate">
			<result name="success">/WEB-INF/jsp/pub/resultPage.jsp</result>
			<result name="error">/WEB-INF/jsp/pub/resultPage.jsp</result>
		</action>
		<action name="userToUpdatePwd" class="userAction" method="userToUpdatePwd">
			<result name="success">/WEB-INF/jsp/biz/user/userUpdatePwd.jsp</result>
		</action>
