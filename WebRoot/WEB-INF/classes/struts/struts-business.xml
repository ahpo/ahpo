			<result name="error">/WEB-INF/jsp/pub/resultPage.jsp</result>
		</action>
		<action name="tblDictionaryDelete" class="tblDictionaryAction"
			method="tblDictionaryDelete">
			<result name="success">/WEB-INF/jsp/pub/resultPage.jsp</result>
		</action>
