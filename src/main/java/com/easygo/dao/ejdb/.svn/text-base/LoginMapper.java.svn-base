package com.easygo.dao.ejdb;

import java.util.List;

import org.apache.ibatis.annotations.Param;
import org.springframework.stereotype.Repository;

import com.easygo.vo.ejdb.Login;
import com.easygo.vo.ejdb.LoginExample;

@Repository
public interface LoginMapper {
	int countByExample(LoginExample example);

	int deleteByExample(LoginExample example);

	int deleteByPrimaryKey(Integer id);

	int insert(Login record);

	int insertSelective(Login record);

	List<Login> selectByExample(LoginExample example);

	/**
	 * 获取分页信息
	 * 
	 * @param example
	 * @param offset
	 *            行偏移
	 * @param pageSize
	 *            分页大小
	 * @return
	 */
	List<Login> selectPageByExample(@Param("example") LoginExample example,
			@Param("offset") int offset, @Param("pageSize") int pageSize);

	Login selectByPrimaryKey(Integer id);

	int updateByExampleSelective(@Param("record") Login record,
			@Param("example") LoginExample example);

	int updateByExample(@Param("record") Login record,
			@Param("example") LoginExample example);

	int updateByPrimaryKeySelective(Login record);

	int updateByPrimaryKey(Login record);
}