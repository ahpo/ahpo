package com.easygo.service.impl.ejdb;

import java.util.List;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.easygo.dao.ejdb.HttpRoadListMapper;
import com.easygo.service.ejdb.HttpRoadListService;
import com.easygo.vo.ejdb.HttpRoadList;
import com.easygo.vo.ejdb.HttpRoadListExample;
import com.pub.service.impl.ServiceImpl;

/**
 * @ClassName: HttpRoadListServiceImpl
 * @Description: TODO(这里用一句话描述这个类的作用)
 * @author zgr
 * @date 2012-9-12 下午2:01:17
 * @version 1.0
 */
@Service("httpRoadListService")
public class HttpRoadListServiceImpl extends ServiceImpl implements
		HttpRoadListService {

	@Resource
	private HttpRoadListMapper httpRoadListMapper;

	public void setHttpRoadListMapper(HttpRoadListMapper httpRoadListMapper) {
		this.httpRoadListMapper = httpRoadListMapper;
	}

	/**
	 * @param example
	 * @return
	 * @see com.easygo.service.ejdb.HttpRoadListService#countByExample(com.easygo.vo.ejdb.HttpRoadListExample)
	 */
	@Override
	public int countByExample(HttpRoadListExample example) {
		logger.info("select count from httproadlist by example...");
		return httpRoadListMapper.countByExample(example);
	}

	/**
	 * @param id
	 * @return
	 * @see com.easygo.service.ejdb.HttpRoadListService#deleteByPrimaryKey(java.lang.Integer)
	 */
	@Override
	public int deleteByPrimaryKey(Integer id) {
		logger.info("delete record from httproadlist by primarykey[" + id
				+ "]...");
		return httpRoadListMapper.deleteByPrimaryKey(id);
	}

	/**
	 * @param record
	 * @return
	 * @see com.easygo.service.ejdb.HttpRoadListService#insertSelective(com.easygo.vo.ejdb.HttpRoadList)
	 */
	@Override
	public int insertSelective(HttpRoadList record) {
		logger.info("insert record into httproadlist...");
		return httpRoadListMapper.insertSelective(record);
	}

	/**
	 * @param example
	 * @param offset
	 * @param pageSize
	 * @return
	 * @see com.easygo.service.ejdb.HttpRoadListService#selectPageByExample(com.easygo.vo.ejdb.HttpRoadListExample,
	 *      int, int)
	 */
	@Override
	public List<HttpRoadList> selectPageByExample(HttpRoadListExample example,
			int offset, int pageSize) {
		logger.info("select page info from httproadlist by example...");
		return httpRoadListMapper
				.selectPageByExample(example, offset, pageSize);
	}

	/**
	 * @param id
	 * @return
	 * @see com.easygo.service.ejdb.HttpRoadListService#selectByPrimaryKey(java.lang.Integer)
	 */
	@Override
	public HttpRoadList selectByPrimaryKey(Integer id) {
		logger.info("select record from httproadlist by primarykey[" + id
				+ "]...");
		return httpRoadListMapper.selectByPrimaryKey(id);
	}

	/**
	 * @param record
	 * @return
	 * @see com.easygo.service.ejdb.HttpRoadListService#updateByPrimaryKey(com.easygo.vo.ejdb.HttpRoadList)
	 */
	@Override
	public int updateByPrimaryKey(HttpRoadList record) {
		logger.info("update record to httproadlist by primarykey["
				+ record.getId() + "]...");
		return httpRoadListMapper.updateByPrimaryKey(record);
	}

}
