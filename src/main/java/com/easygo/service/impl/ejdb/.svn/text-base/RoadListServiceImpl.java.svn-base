package com.easygo.service.impl.ejdb;

import java.util.List;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.easygo.dao.ejdb.RoadListMapper;
import com.easygo.service.ejdb.RoadListService;
import com.easygo.vo.ejdb.RoadList;
import com.easygo.vo.ejdb.RoadListExample;
import com.pub.service.impl.ServiceImpl;

@Service("roadListService")
public class RoadListServiceImpl extends ServiceImpl implements RoadListService {

	@Resource
	private RoadListMapper roadListMapper;

	public RoadListMapper getRoadListMapper() {
		return roadListMapper;
	}

	public void setRoadListMapper(RoadListMapper roadListMapper) {
		this.roadListMapper = roadListMapper;
	}

	public int countByExample(RoadListExample example) {
		logger.info("get count by example...");
		return roadListMapper.countByExample(example);
	}

	public int deleteByPrimaryKey(Integer id) {
		logger.info("delete from roadList by id["+id+"]...");
		return roadListMapper.deleteByPrimaryKey(id);
	}

	public int insertSelective(RoadList record) {
		logger.info("insert record into roadList...");
		return roadListMapper.insertSelective(record);
	}

	public List<RoadList> selectPageByExample(RoadListExample example,
			int offset, int pageSize) {
		// TODO Auto-generated method stub
		logger.info("select page info from roadList...");
		return roadListMapper.selectPageByExample(example, offset, pageSize);
	}

	public RoadList selectByPrimaryKey(Integer id) {
		logger.info("get roadList by primarykey["+id+"]...");
		return roadListMapper.selectByPrimaryKey(id);
	}

	public int updateByPrimaryKey(RoadList record) {
		logger.info("update roadList ["+record.getId()+"]...");
		return roadListMapper.updateByPrimaryKey(record);
	}

}
